  "devDependencies": {
    "@angular/cli": "10.1.7",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@nestjs/schematics": "7.1.2",
    "@nestjs/testing": "7.4.4",
    "@nrwl/cli": "10.3.1",
    "@nrwl/eslint-plugin-nx": "10.3.1",
    "@nrwl/jest": "10.3.1",
    "@nrwl/nest": "10.3.1",
