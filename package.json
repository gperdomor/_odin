  "private": true,
  "dependencies": {
    "@algoan/nestjs-pagination": "3.4.0",
    "@nestjs/common": "7.4.4",
    "@nestjs/config": "0.5.0",
    "@nestjs/core": "7.4.4",
    "@nestjs/mapped-types": "0.1.0",
    "@nestjs/platform-express": "7.4.4",
